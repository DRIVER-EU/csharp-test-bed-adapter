<?xml version="1.0" standalone="yes"?>
<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="settings">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="client.id" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="heartbeat.interval" type="xs:int" minOccurs="1" maxOccurs="1" />
        <xs:element name="security.protocol" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="security.ca.path" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="security.keystore.path" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="security.keystore.password" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="broker.url" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="schema.url" type="xs:string" minOccurs="1" maxOccurs="1" />
        <xs:element name="send.sync" type="xs:boolean" minOccurs="1" maxOccurs="1" />
        <xs:element name="retry.count" type="xs:int" minOccurs="1" maxOccurs="1" />
        <xs:element name="retry.time" type="xs:int" minOccurs="1" maxOccurs="1" />
        <xs:element name="direct.connect" type="xs:boolean" minOccurs="1" maxOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="1">
        <xs:element ref="settings" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>